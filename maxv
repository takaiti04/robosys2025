import sys

values = []

for line in sys.stdin:
    raw = line.rstrip("\n")
    text = raw.strip()


    if text == "":
        print(f"エラー:空行は不可 -- {raw}", file=sys.stderr)
        sys.exit(1)

    try:
        num = float(text)
    except ValueError:
        print(f"エラー:数値ではない -- {raw}", file=sys.stderr)
        sys.exit(1)

    values.append(num)

if len(values) == 0:
    print("エラー:数値が入力されてない", file=sys.stderr)
    sys.exit(1)

m = max(values)

if m.is_integer():
    print(int(m))
else:
    print(m)

